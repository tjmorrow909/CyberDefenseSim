    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^8.0.3",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "vite": "^5.4.14",
    "vitest": "^2.1.8"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
